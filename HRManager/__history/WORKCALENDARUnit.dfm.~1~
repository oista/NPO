  object ADQuery1: TADQuery
    Connection = DataModule1.ADConnection1
    SQL.Strings = (
      
        'select substr(DAY_DATE,1,2) dayno,RN,ISHOLIDAY,DAY_LENGTH_H,DAY_' +
        'LENGTH_SHORT_H,DAY_LENGTH_M,DAY_LENGTH_SHORT_M , DAY_DATE,'
      'trim(to_char(day_date,'#39'day'#39') ) day_name'
      'from OTIZ$TWORKINGCALENDAR where '
      'CALENDAR_YEAR=:year AND'
      'trim(to_char(day_date,'#39'month'#39'))=:mon'
      'ORDER by DAY_DATE')
    Left = 200
    Top = 40
    ParamData = <
      item
        Name = 'YEAR'
        DataType = ftString
        ParamType = ptInput
        Value = Null
      end
      item
        Name = 'MON'
        DataType = ftString
        ParamType = ptInput
        Value = Null
      end>
